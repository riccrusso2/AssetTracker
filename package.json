{
  "name": "portfolio-tracker",
  "version": "1.0.0",
  "description": "React + Express portfolio tracker",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "node server/server.js",
    "client": "DANGEROUSLY_DISABLE_HOST_CHECK=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "proxy": "http://localhost:3001",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide-react": "0.539.0",
    "node-fetch": "^2.6.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-is": "19.1.1",
    "react-scripts": "^5.0.1",
    "recharts": "3.1.2",
    "yahoo-finance2": "2.13.3"
  },
  "devDependencies": {
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "concurrently": "^9.2.0",
    "loader-utils": "3.2.1",
    "typescript": "^4.9.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
